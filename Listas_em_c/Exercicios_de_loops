//Questão 17
/**/
#include "stdio.h"
void main(){
    int codigo;
    float preco;
    float preco_novo;
    float media_preco_soma = 0;
    float media_preco_novo_soma = 0;
    float media_preco_novo;
    float media_preco;
    int total = 0;
    do{
        printf("Digite o codigo e o preco do produto nesse formato -> codigo/preco: ");
        scanf("%i/%f", &codigo, &preco);
        if(codigo > 0){

        preco_novo = (preco * 20 / 100) + preco;
        printf("o codigo do produto eh : %i \n", codigo);
        printf("o preco novo eh : %.2f\n", preco_novo);
        
        media_preco_soma += preco;
        media_preco_novo_soma += preco_novo;
        total++;
        }

    }while(codigo >= 0);
media_preco_novo = media_preco_novo_soma / total;
media_preco = media_preco_soma / total;
printf("A media dos precos com aumento eh: R$%f\n", media_preco_novo);
printf("A media dos precos sem aumento eh: R$%f\n", media_preco);

}

